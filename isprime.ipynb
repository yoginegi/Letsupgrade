def isprime(num):                                                                 
    for i in range(2, num):                                                       
        if num % i == 0:                                                          
            print("The number " + str(num) + " is not a prime number.")           
            break                                                                 
    else:                                                                         
        print("The number " + str(num) + " is a prime number.")                   
                                                                                  
                                                                                  
isprime(111)                                                                      
